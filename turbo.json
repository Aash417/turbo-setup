{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "dependsOn": []
    },
    "db:migrate": {
      "dependsOn": []
    },
    "db:push": {
      "dependsOn": []
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    }
  }
}